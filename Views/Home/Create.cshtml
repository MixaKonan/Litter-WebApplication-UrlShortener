@model Litter.Models.LitterModel
@{ ViewData["Title"] = "Create"; }

<div class="text-center">
    <form asp-action="create" asp-controller="home">
        <div class="form-group">
            <label asp-for="Url" class="control-label">Url</label>
            <input type="text" asp-for="Url" class="form-control" />
            <input type="hidden" asp-for="GeneratedKey" value="@Litter.Support.KeyGenerator.Generate()" class="form-control" />
        </div>
        <div class="form-group">
            <input type="submit" value="Отправить" class="btn btn-default" />
        </div>
    </form>
</div>